{
  "name": "swiftbite",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "test": "jest --watchAll",
    "lint": "eslint . --fix",
    "types": "bunx supabase gen types typescript --project-id 'ffbbrrfdghbvuajheulg' --schema public > database.types.ts",
    "format": "prettier --write .",
    "preinstall": "bun run types",
    "postinstall": "find ./node_modules/react-native-reanimated-skeleton -type f -exec sed -i '' -e 's/import LinearGradient/import { LinearGradient }/g' -e 's/react-native-linear-gradient/expo-linear-gradient/g' {} +",
    "build:prebuild": "bunx expo prebuild",
    "build:pod-install": "bunx pod-install",
    "build:internal": "eas build --platform ios --profile preview --non-interactive --local",
    "build:simulator": "eas build --platform ios --profile simulator --non-interactive --local",
    "build:production": "eas build --platform ios --profile production --non-interactive --local",
    "build:development": "eas build --platform ios --profile development --non-interactive --local",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.12",
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@expo-google-fonts/open-sans": "^0.3.0",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.0.1",
    "@krosben/react-native-gravatar": "^0.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-picker/picker": "2.11.0",
    "@sentry/react-native": "~6.14.0",
    "@supabase/supabase-js": "^2.49.8",
    "@tanstack/react-query": "^5.77.2",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^53.0.9",
    "expo-apple-authentication": "~7.2.4",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.4",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "prettier-plugin-sort-imports": "^1.8.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.4",
    "react-native": "0.79.2",
    "react-native-barcodes-detector": "^0.1.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-charts": "^1.4.61",
    "react-native-health": "^1.19.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-reanimated-skeleton": "^1.6.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-ui-datepicker": "^3.1.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-view-shot": "~4.0.3",
    "react-native-vision-camera": "^4.6.4",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "react-native-worklets-core": "^1.5.0",
    "vision-camera-base64-v3": "git+https://github.com/schotsl/vision-camera-base64",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^9.27.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.5",
    "prettier": "^3.5.3",
    "react-test-renderer": "19.1.0",
    "supabase": "^2.23.4",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@ai-sdk/react",
          "@hookform/resolvers",
          "@krosben/react-native-gravatar",
          "prettier-plugin-sort-imports",
          "base64-arraybuffer",
          "react-native-barcodes-detector",
          "react-native-reanimated-skeleton",
          "react-native-health",
          "react-native-swipe-list-view",
          "vision-camera-base64-v3"
        ]
      }
    }
  },
  "overrides": {
    "@expo/config-plugins": "~10.0.0"
  },
  "trustedDependencies": [
    "@sentry/cli",
    "unrs-resolver"
  ]
}
