{
  "name": "swiftbite",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "test": "jest --watchAll",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "types": "supabase gen types typescript --project-id 'ffbbrrfdghbvuajheulg' --schema public > database.types.ts",
    "format": "prettier --write .",
    "preinstall": "bun run types",
    "postinstall": "find ./node_modules/react-native-reanimated-skeleton -type f -exec sed -i '' -e 's/import LinearGradient/import { LinearGradient }/g' -e 's/react-native-linear-gradient/expo-linear-gradient/g' {} +",
    "build:internal": "eas build --platform ios --profile preview --non-interactive",
    "build:simulator": "eas build --platform ios --profile simulator --non-interactive",
    "build:production": "eas build --platform ios --profile production --non-interactive",
    "build:development": "eas build --platform ios --profile development --non-interactive"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.3",
    "@expo-google-fonts/open-sans": "^0.3.0",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.48.1",
    "@tanstack/react-query": "^5.67.1",
    "base64-arraybuffer": "^1.0.2",
    "expo": "^52.0.46",
    "expo-apple-authentication": "~7.1.3",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.19",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.7",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "prettier-plugin-sort-imports": "^1.8.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.9",
    "react-native-barcodes-detector": "^0.1.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-health": "^1.19.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-skeleton": "^1.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "prettier": "^3.5.3",
    "react-test-renderer": "18.3.1",
    "supabase": "^2.22.4",
    "typescript": "^5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "base64-arraybuffer"
        ]
      }
    }
  }
}
