{
  "expo": {
    "name": "Swiftbite",
    "slug": "swiftbite",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Swiftbite makes food tracking easy with effortless input and no hassle.",
    "icon": "./assets/images/icon.png",
    "scheme": "com.sjorsvanholst.swiftbite",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sjorsvanholst.swiftbite",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Read health data",
        "NSHealthUpdateUsageDescription": "Update health data",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.healthkit": true
      },
      "ios": {
        "usesAppleSignIn": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#d0d0d0"
      }
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "react-native-health",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d0d0d0"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "937d1ff7-210d-4190-8d21-3beebc1a979b"
      }
    }
  }
}
